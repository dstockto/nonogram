{
    "name": "dstockton/bestfit",
    "require": {
        "phpunit/phpunit": "^5.5",
        "illuminate/support": "^5.3",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "authors": [
        {
            "name": "David Stockton",
            "email": "dave@davidstockton.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FitFinder\\": "src/FitFinder"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit tests"
    },
    "autoload-dev": {
        "psr-4": {
            "FitFinderTest\\": "tests/"
        }
    }
}
