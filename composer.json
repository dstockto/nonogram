{
    "name": "dstockton/bestfit",
    "require": {
        "phpunit/phpunit": "^9.5.25",
        "illuminate/support": "^v9.52.13",
        "squizlabs/php_codesniffer": "^3.7.2"
    },
    "authors": [
        {
            "name": "David Stockton",
            "email": "dave@davidstockton.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FitFinder\\": "src/FitFinder"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit tests"
    },
    "autoload-dev": {
        "psr-4": {
            "FitFinderTest\\": "tests/"
        }
    }
}
